{
   "name": "crowdfundr",
   "private": true,
   "version": "0.0.0",
   "type": "module",
   "scripts": {
      "dev": "concurrently \"npm run vite\" \"npm run server\" \"npm run sass\"",
      "vite": "vite",
      "server": "json-server --watch db.json -m ./node_modules/json-server-auth --port 5000",
      "sass": "sass --watch src/styles/main.scss src/styles/main.css",
      "build": "vite build",
      "preview": "vite preview"
   },
   "devDependencies": {
      "concurrently": "^9.2.1",
      "json-server": "^0.17.4",
      "json-server-auth": "^2.1.0",
      "sass": "^1.91.0",
      "vite": "^7.1.2"
   },
   "dependencies": {
      "@fortawesome/fontawesome-free": "^7.0.1"
   }
}
